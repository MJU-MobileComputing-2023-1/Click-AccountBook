<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 테이블 레이아웃 스타일 -->
    <style name="TableLayoutStyle">
        <item name="android:stretchColumns">*</item>  <!-- 테이블의 열이 화면의 너비에 맞게 조정됨 -->
        <item name="android:background">#000000</item> <!-- 테이블 배경 색상 설정 -->
    </style>

    <!-- 테이블 행 스타일 -->
    <style name="TableRowStyle">
        <item name="android:layout_margin">1dp</item> <!-- 행과 행 사이에 여백 설정 -->
        <item name="android:background">#000000</item> <!-- 행 배경 색상 (테두리 색상) 설정 -->
    </style>

    <!-- 텍스트 뷰 스타일 -->
    <style name="TextViewStyle">
        <item name="android:background">#FFFFFF</item> <!-- 셀 배경 색상 설정 -->
    </style>

</resources>